#include <iostream>
using namespace std;

int main() {
    char ch;
    
    cout << "Enter a character: " << endl;
    cin >> ch;

    // Check if the entered character is a lowercase letter
    if (ch >= 'a' && ch <= 'z') {
        cout << "Lowercase letter" << endl;
    } else if (ch >= 'A' && ch <= 'Z') {
        cout << "Uppercase letter" << endl;
    } else if (ch >= '0' && ch <= '9') {
        cout << "Numeric letter" << endl;
    } else {
        cout << "Symbol" << endl;
    }

    return 0;
}

