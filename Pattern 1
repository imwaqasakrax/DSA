#include <iostream>

int main() {
    int n;
    int i = 1;
    
    std::cin >> n;  
    
    while (i <= n) {  
        int j = 1;
        
        while (j <= n) {
            std::cout << '*';  
            j++;  
        }
        
        std::cout << std::endl;  
        i++;  
    }
    

}
